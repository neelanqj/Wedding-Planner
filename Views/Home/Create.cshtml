@{
    ViewData["Title"] = "Home Page";
    ViewData["Heading"] = "Plan Your Wedding";
}

<partial name="/Views/Shared/_Navigation.cshtml" />

<div class="row">
    <div class="col-md-12">
        @model Wedding
        <form action="/create" method="POST">
            <label asp-for="Wedder1"></label>
            <input asp-for="Wedder1" /><br/>
            <label asp-for="Wedder2"></label>
            <input asp-for="Wedder2" /><br/>
            
            <label asp-for="Date"></label>
            <input type="date" asp-for="Date" /><br/>

            <label asp-for="Address"></label>
            <input asp-for="Address" />

            <input type="submit" value="Create" />
        </form>
    </div>
</div>